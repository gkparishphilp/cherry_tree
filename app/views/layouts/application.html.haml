!!!strict
%html
	%head
		= render 'layouts/meta'
		= init_tinymce_hammer_if_required
	%body
		#container
			= render 'shared/header'
			#bd
				.column1
					= render 'shared/flash', :flash => flash 
					.pad
						= yield :left_column
				.column2
					.pad
						= yield :right_column
		
			= render 'shared/footer'
		= render 'shared/analytics'
