%div[child]
	= render "children/sub_nav", :child => child

	.child_dash.clearfix
		.floatted_box
			%h3
				Goals
			%div.text
				
		
		.floatted_box
			%h3
				Rewards
			%div.text

		.floatted_box
			%h3
				Send a Note
			%div.text
		
		.floatted_box
			%h3
				Recent Activities
			%div.text
				- for activity in Activity.feed( child )
					= render activity
		